        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "id": 3,
      "title": "Study Session Metrics",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(study_sessions_total[1h])",
          "legendFormat": "Study Sessions/Hour",
          "refId": "A"
        },
        {
          "expr": "avg(study_session_duration_seconds)",
          "legendFormat": "Avg Session Duration",
          "refId": "B"
        },
        {
          "expr": "sum(rate(cards_reviewed_total[1h]))",
          "legendFormat": "Cards Reviewed/Hour",
          "refId": "C"
        },
        {
          "expr": "avg(study_session_completion_rate)",
          "legendFormat": "Session Completion Rate",
          "refId": "D"
        }
      ]
    },
